package baekjoon;

import java.util.Random;
import java.util.Scanner;

public class 돌_게임 {
	
	static void rockGame(int n) {		
		Random rand = new Random();
		
		String winner;
		int sk;
		int cy;		
		sk = rand.nextInt(3);
		cy = rand.nextInt(3);
		

		
		System.out.println("winner : " + winner);
		
	}

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		rockGame(n);
		
	}
	
}
